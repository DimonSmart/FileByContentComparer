<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>

    <PackageId>DimonSmart.FileByContentComparer</PackageId>
    <Version>1.0.0</Version>
    <Authors>Dmitry Dorogoy</Authors>
    <Company>DimonSmart</Company>

    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <RootNamespace>DimonSmart.$(MSBuildProjectName.Replace(" ", "_"))</RootNamespace>
    <PackageTags>FileContent;Compare;BinaryCompare</PackageTags>

	<Description>This package provide file by content comparision functionality with FileByContent class and IFileByContent interface</Description>

	<RepositoryUrl>https://github.com/DimonSmart/FileByContentComparer</RepositoryUrl>
	<RepositoryType>git</RepositoryType>
	<PackageLicenseExpression>0BSD</PackageLicenseExpression>
  
  </PropertyGroup>

</Project>
